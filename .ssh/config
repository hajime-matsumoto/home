# エイリアス名
Host gw
    Hostname gw.hazime.org
    User kurari
    Port 22
    IdentityFile ~/.ssh/kurari_rsa
    ForwardAgent yes

Host dev
    User kurari
    Hostname dev.hazime.org
    Port 22
    IdentityFile ~/.ssh/kurari_rsa
    ForwardAgent yes

Host git
    User git
    Hostname dev.hazime.org
    Port 22
    IdentityFile ~/.ssh/kurari_rsa
    ForwardAgent yes

Host web
    ProxyCommand ssh -q gw nc -q0 web 22
    ForwardAgent yes
